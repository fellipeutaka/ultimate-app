{
  "name": "@ultimate/mobile",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --output-dir ./build --platform all",
    "eas-build-pre-install": "npm install --global pnpm@7.x",
    "eas-build-post-install": "pnpm run -w build:mobile",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest"
  },
  "dependencies": {
    "expo": "~47.0.12",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-web": "~0.18.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@ultimate/tsconfig": "workspace:*",
    "@ultimate/eslint-config": "workspace:*",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-root-import": "^6.6.0",
    "react-test-renderer": "^18.2.0"
  }
}
